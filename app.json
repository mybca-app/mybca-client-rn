{
  "expo": {
    "name": "myBCA",
    "description": "The all-in-one BCA student app",
    "slug": "mybca-client-rn",
    "version": "2.0",
    "orientation": "default",
    "icon": "./assets/images/icons/icon.png",
    "scheme": "mybcaclientrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "githubUrl": "https://github.com/mybca-app/mybca-client-rn",
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/images/icons/ios/light.png",
        "dark": "./assets/images/icons/ios/dark.png",
        "tinted": "./assets/images/icons/ios/tinted.png"
      },
      "bundleIdentifier": "link.mybca.mobileclient",
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "ITSAppUsesNonExemptEncryption": false,
        "FirebaseAppDelegateProxyEnabled": true
      },
      "googleServicesFile": "firebase/GoogleService-Info.plist",
      "appStoreUrl": "https://apps.apple.com/us/app/mybca/id6756277506"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fffef0",
        "foregroundImage": "./assets/images/icons/android/foreground.png",
        "monochromeImage": "./assets/images/icons/android/foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "link.mybca.mobileclient",
      "googleServicesFile": "firebase/google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "forceStaticLinking": ["RNFBApp", "RNFBMessaging"]
          }
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fffef0",
          "dark": {
            "backgroundColor": "#0a101d"
          }
        }
      ],
      "expo-web-browser",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-quick-actions"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bfe6de8-4056-4ee5-8b7c-e84a43f0866e"
      }
    }
  }
}
